<view>
  <view class="top-image"><image class="top-image" src="https://img3.doubanio.com/view/movie_poster_cover/lpst/public/p2462515303.jpg" /></view>
</view>
<view class="service-pd"style="height:20px;"></view>
<view class="weui-cells weui-cells_after-title" wx:for="{{routes}}" >
     <navigator url="{{item.route}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
            <image src="{{item.icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
        </view>
        <view class="weui-cell__bd">{{item.text}}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
</view>